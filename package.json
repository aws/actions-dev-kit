    "url": "https://aws.amazon.com"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "webpack-cli": "^4.9.1"
  },
  "resolutions": {
    "projen": "0.71.112",
    "typescript": "^4.9.5"
  },
  "engines": {
    "npm": "6.14.13"
  }
