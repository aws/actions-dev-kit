{
  "name": "adk",
  "private": true,
  "devDependencies": {
    "@esbuild/linux-x64": "0.19.3",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "*",
    "eslint-config-node": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "husky": "^4.3.8",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typedoc": "^0.23.13",
    "typescript": "^4.9.5",
    "webpack": "^5.62.1"
  },
  "scripts": {
    "build": "PROJEN_DISABLE_POST=1 yarn workspaces foreach --topological-dev -v run build",
    "projen": "PROJEN_DISABLE_POST=1 yarn workspaces foreach --topological-dev -v run projen",
    "api-ref": "typedoc",
    "api-ref-pre-release": "typedoc --out pre-release-docs",
    "generate-attributions": "ts-node ./scripts/generate-attributions",
    "delete-remote-tags": "ts-node ./scripts/delete-remote-tags.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn build"
    }
  },
  "workspaces": {
    "packages": [
      "packages/adk/*",
      "packages/entities/*",
      "packages/utils/*",
      "packages/workflows/*"
    ]
  },
  "repository": {
    "url": "git+https://github.com/aws/actions-dev-kit",
