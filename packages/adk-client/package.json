{
  "name": "@quokka/adk-client",
  "version": "0.40.0",
  "main": "dist/lib/fusi.js",
  "types": "dist/lib/fusi.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && cp -r lib/graphql dist/graphql/",
    "codegen": "graphql-codegen && gqlg --schemaFilePath ./schema.graphql --destDirPath lib/graphql",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix '**/*.ts'"
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.8",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@types/jest": "^29.2.0",
    "@types/node-fetch": "^2.6.2",
    "gql-generator": "~1.0.18",
    "graphql": "^16.6.0",
    "typescript-json-schema": "^0.54.0"
  },
  "npm-pretty-much": {
    "runTest": "never"
  }
}
